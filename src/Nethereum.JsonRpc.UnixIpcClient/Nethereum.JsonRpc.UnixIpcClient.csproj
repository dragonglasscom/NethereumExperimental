<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.props))\dir.props" />
  <PropertyGroup>
    <Description>Nethereum JsonRpc.UnixIpcClient Ethereum / Netherum Unix Ipc Client provider</Description>
    <Copyright>Juan Blanco</Copyright>
    <AssemblyTitle>Nethereum.JsonRpc.UnixIpcClient</AssemblyTitle>
    <VersionPrefix>$(NethereumVersion)</VersionPrefix>
    <Authors>Juan Blanco, djsowa</Authors>
    <TargetFrameworks>netstandard1.3</TargetFrameworks>
    <AssemblyName>Nethereum.JsonRpc.UnixIpcClient</AssemblyName>
    <PackageId>Nethereum.JsonRpc.UnixIpcClient</PackageId>
    <PackageTags>Json;RPC</PackageTags>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Nethereum/Nethereum/master/LICENSE.md</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Nethereum.JsonRpc.Client\Nethereum.JsonRpc.Client.csproj" />
    <PackageReference Include="EdjCase.JsonRpc.Client" Version="1.3.1" />
  </ItemGroup>
  
</Project>
